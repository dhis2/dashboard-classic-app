{
  "name": "dashboard-classic",
  "version": "28.0.9",
  "description": "DHIS2 Legacy Dashboard",
  "main": "src/app.js",
  "scripts": {
    "coverage": "babel-node node_modules/.bin/isparta cover --root ./src --report text --report html --report lcov node_modules/mocha/bin/_mocha -- --reporter dot --require config/setup.js --recursive test",
    "prebuild": "rm -rf build && mkdir build && d2-manifest package.json manifest.webapp",
    "build": "npm run prebuild && webpack && npm run manifest",
    "postbuild": "cp -rv javascript plugin style icon*.png i18n manifest.webapp build/ && grunt",
    "manifest": "d2-manifest package.json build/manifest.webapp",
    "dist": "npm run build && npm run postbuild && cd build && zip -r dashboard-classic.zip * && cd ..",
    "start": "webpack-dev-server"
  },
  "keywords": [
    "DHIS2",
    "App"
  ],
  "author": "Viet Nguyen <viet@dhis2.org>",
  "license": "BSD-3-Clause",
  "dependencies": {
    "script-loader": "^0.7.0"
  },
  "devDependencies": {
    "babel-core": "^6.7.2",
    "babel-eslint": "^6.0.4",
    "babel-loader": "^6.2.4",
    "babel-polyfill": "^6.7.2",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "d2": "24.0.x",
    "d2-manifest": "^1.0.0",
    "d2-ui": "24.0.x",
    "d2-utilizr": "^0.2.9",
    "grunt": "^1.0.1",
    "grunt-string-replace": "^1.3.1",
    "webpack": "^1.12.14"
  },
  "pre-commit": [],
  "directories": {
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "git+github.com:dhis2/dashboard-app"
  },
  "homepage": "https://github.com/dhis2/dashboard-app",
  "manifest.webapp": {
    "name": "Dashboard Classic",
    "description": "DHIS2 Legacy Dashboard App",
    "icons": {
      "48": "icon.png"
    },
    "developer": {
      "url": "",
      "name": "DHIS2"
    },
    "installs_allowed_from": null,
    "launchUrl": null,
    "dhis2": {
      "apiVersion": "28"
    },
    "activities": {
      "dhis": {
        "href": "../../../"
      }
    }
  }
}
